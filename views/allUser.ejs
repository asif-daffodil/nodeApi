<%- include('./header.ejs') -%>
<body>
  <div class="container mt-5">
    <div class="row">
      <div class="col-md-12 m-auto">
        <table class="table table-striped table-hover">
          <tr>
            <th>ID</th>
            <th>Image</th>
            <th>Name</th>
            <th>Gender</th>
            <th>Phone</th>
            <th>Address</th>
          </tr>
          <% users.forEach(user => { %>
          <tr>
            <td><%= user._id %></td>
            <td>
              <img src="<%= user.picture %>" alt="" class="w-25" />
            </td>
            <td><%= user.name %></td>
            <td><%= user.gender %></td>
            <td><%= user.contact %></td>
            <td><%= user.address %></td>
          </tr>
          <% }); %>
        </table>
      </div>
    </div>
  </div>
</body>
<%- include('./footer.ejs') -%>
